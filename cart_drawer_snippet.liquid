    },
    loop: true,
  });
}




(function() {
  const originalFetch = window.fetch;
  window.fetch = async function(...args) {
    const response = await originalFetch.apply(this, args);
    if (args[0].includes('/cart/add')) {
      

refreshCartDrawer();
document.body.style.overflow = 'hidden';
        
    }
    return response;
  };
})();






  const cartDrawer = document.querySelector(".custom_cart_drawer");

  if (!cartDrawer) return;

  cartDrawer.addEventListener("click", async function (e) {
    const button = e.target.closest(".quantity__button");
    if (!button) return;

    e.preventDefault();

    const itemKey = button.dataset.id; // Shopify line item key
    let newQuantity = parseInt(button.dataset.quantity);

    // ðŸš« Prevent going below 1
    if (newQuantity < 1) {
      newQuantity = 1;
    }

    try {
      const response = await fetch("/cart/change.js", {
        method: "POST",
        headers: {
          "Content-Type": "application/json",
          Accept: "application/json",
        },
        body: JSON.stringify({
          id: itemKey,
          quantity: newQuantity,
        }),
      });

      const data = await response.json();

      // âœ… Update only the clicked item's wrapper
      const wrapper = button.closest(".quantity_wrapper");
      const qtyBox = wrapper.querySelector(".cart_quantity");
      if (qtyBox) qtyBox.textContent = newQuantity;

      // Update +/- data for next clicks (min stays 1)
      const minusBtn = wrapper.querySelector(".quantity-minus");
      const plusBtn = wrapper.querySelector(".quantity-plus");

      if (minusBtn) {
        minusBtn.dataset.quantity = Math.max(1, newQuantity - 1);

        // ðŸ”’ Disable minus button if qty = 1
        if (newQuantity <= 1) {
          minusBtn.setAttribute("disabled", "true");
          minusBtn.classList.add("disabled");
        } else {
          minusBtn.removeAttribute("disabled");
          minusBtn.classList.remove("disabled");
        }
      }

      if (plusBtn) plusBtn.dataset.quantity = newQuantity + 1;

      // Refresh totals / discounts / shipping etc.
      if (typeof refreshCartDrawer === "function") {
     refreshCartDrawer();
      }
    } catch (error) {
      console.error("Error updating cart quantity:", error);
    }
  });
});



